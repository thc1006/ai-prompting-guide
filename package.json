{
  "name": "ai-prompting-guide",
  "version": "1.0.0",
  "private": true,
  "description": "A comprehensive guide to AI prompting techniques and best practices",
  "keywords": [
    "ai",
    "artificial-intelligence",
    "prompting",
    "prompt-engineering",
    "guide",
    "docusaurus",
    "documentation",
    "machine-learning",
    "nlp",
    "chatgpt",
    "llm"
  ],
  "homepage": "https://tingy.github.io/ai-prompting-guide/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tingy/ai-prompting-guide.git"
  },
  "bugs": {
    "url": "https://github.com/tingy/ai-prompting-guide/issues"
  },
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --host 0.0.0.0 --port 3000",
    "dev": "npm run start",
    "build": "docusaurus build",
    "build:analyze": "npm run build -- --bundle-analyzer",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "deploy:gh": "GIT_USER=${GIT_USER} npm run deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve --host 0.0.0.0 --port 3000",
    "serve:build": "npm run build && npm run serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "precommit": "npm run typecheck && npm run lint && npm run format:check",
    "prebuild": "npm run clear",
    "postbuild": "echo \"Build completed successfully!\"",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "@docusaurus/core": "^3.8.1",
    "@docusaurus/preset-classic": "^3.8.1",
    "@docusaurus/theme-mermaid": "^3.8.1",
    "@mdx-js/react": "^3.0.1",
    "clsx": "^2.1.1",
    "prism-react-renderer": "^2.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remark-math": "^6.0.0",
    "rehype-katex": "^7.0.1"
  },
  "devDependencies": {
    "@docusaurus/eslint-plugin": "^3.8.1",
    "@docusaurus/module-type-aliases": "^3.8.1",
    "@docusaurus/tsconfig": "^3.8.1",
    "@docusaurus/types": "^3.8.1",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.3.3",
    "typescript": "~5.5.4",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0",
    "npm": ">=8.0"
  },
  "packageManager": "npm@10.0.0"
}